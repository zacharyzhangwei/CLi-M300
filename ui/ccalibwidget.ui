{
    width:8px;
    background:rgba(64,64,64,50%);
	border-bottom-right-radius:8px;
	border-top-right-radius:8px;
    min-height:0;
}
QScrollBar::handle:vertical:hover
{
    width:8px;
    background:rgba(64,64,64,70%);   /*  鼠标放到滚动条上的时候，颜色变深*/
	border-bottom-right-radius:8px;
	border-top-right-radius:8px;
    min-height:0;
}
