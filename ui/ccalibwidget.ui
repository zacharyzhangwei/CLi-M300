/*  设置垂直滚动条基本样式*/
QScrollBar:vertical
{
    width:8px;
	background-color: rgb(233, 236, 245);
    margin:00px,0px,0px,0px;
    padding-top:15px;   /* 留出9px给上面和下面的箭头*/
    padding-bottom:5px;
}
QScrollBar::handle:vertical
{
    width:8px;
    background:rgba(64,64,64,50%);
	border-bottom-right-radius:8px;
	border-top-right-radius:8px;
    min-height:0;
}
QScrollBar::handle:vertical:hover
{
    width:8px;
    background:rgba(64,64,64,70%);   /*  鼠标放到滚动条上的时候，颜色变深*/
	border-bottom-right-radius:8px;
	border-top-right-radius:8px;
    min-height:0;
}
QScrollBar::add-page:vertical,QScrollBar::sub-page:vertical   /* 当滚动条滚动的时候，上面的部分和下面的部分*/
{
    background:rgba(0,0,0,0%);
    border-radius:4px;
}
QScrollBar::add-line:vertical
{
    height: 0px;
    subcontrol-origin:margin;
}

QScrollBar::up-arrow:vertical 
{
    subcontrol-origin: margin;
    height: 0px;
    border:0 0 0 0;
    border-image: url(:images/6.png);
}
QScrollBar::sub-line:vertical 
{
    height: 0px;
    subcontrol-origin:margin;
}

QScrollBar::down-arrow:vertical 
{
    subcontrol-origin: margin;
    height: 0px;
    border-image: url(:images/7.png);
}

/*水平滚动条*/
QScrollBar:horizontal {
	background-color: rgb(210, 210, 210);
      height: 10px;
      margin: 0px 0px 0 0px;
  }
  QScrollBar::handle:horizontal {
      background: rgb(64, 64, 64,50%);
	border-bottom-left-radius:8px;
	border-bottom-right-radius:8px;
      min-width: 0px;
  }

QScrollBar::handle:horizontal:hover
{
    width:8px;
    background:rgba(64,64,64,70%);   /*  鼠标放到滚动条上的时候，颜色变深*/
	border-bottom-left-radius:8px;
	border-bottom-right-radius:8px;
    min-height:0;
}
  QScrollBar::add-line:horizontal {
      width: 0px;
      subcontrol-position: right;
      subcontrol-origin: margin;
  }

  QScrollBar::sub-line:horizontal {
      width: 0px;
      subcontrol-position: left;
      subcontrol-origin: margin;
  }
 QScrollBar:left-arrow:horizontal, QScrollBar::right-arrow:horizontal {
      border: 2px solid grey;
      width: 3px;
      height: 3px;
      background: white;
  }

